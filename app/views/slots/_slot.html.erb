<div class='slot' id='slot_<%= slot.id %>'>
  <h3><%= slot.name.split(' ', 3).last %></h3>
  <div class='list-group'>
    <% slot.talks.each do |talk| %>
      <div class='list-group-item'>
        <%= render partial: 'talks/talk', locals: { talk: talk, current_user: current_user } %>
      </div>
    <% end %>
  </div>
</div>
